{
  "name": "qusic-api",
  "version": "1.0.0",
  "repository": "https://github.com/JonasTriki/qusic-api.git",
  "author": "Jonas Triki <trikijonas@gmail.com>",
  "license": "MIT",
  "scripts": {
    "lint": "eslint . --ext .js,.ts",
    "prebuild": "yarn lint",
    "build": "tsc",
    "serve": "yarn build && firebase serve --only functions",
    "shell": "yarn build && firebase functions:shell",
    "start": "nodemon",
    "deploy": "firebase deploy --only functions",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": "10"
  },
  "main": "lib/main.js",
  "dependencies": {
    "argon2": "^0.25.1",
    "cors": "^2.8.5",
    "express": "^4.17.3",
    "express-validator": "^6.3.0",
    "firebase-admin": "^8.6.0",
    "firebase-functions": "^3.3.0",
    "geofirestore": "^3.3.1",
    "module-alias": "^2.2.2",
    "morgan": "^1.9.1",
    "uuid": "^3.3.3",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@types/cors": "^2.8.6",
    "@types/express": "^4.17.2",
    "@types/morgan": "^1.7.37",
    "@typescript-eslint/eslint-plugin": "^2.14.0",
    "@typescript-eslint/parser": "^2.14.0",
    "eslint": "^6.8.0",
    "eslint-config-airbnb-base": "^14.0.0",
    "eslint-config-prettier": "^6.9.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^2.0.0",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-json": "^2.0.1",
    "eslint-plugin-prettier": "^3.1.2",
    "firebase-functions-test": "^0.1.6",
    "husky": "^3.1.0",
    "nodemon": "^2.0.2",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.4",
    "tslint": "^5.12.0",
    "typescript": "^3.7.4"
  },
  "_moduleAliases": {
    "_responses": "lib/responses",
    "_logger": "lib/logger",
    "_firebase": "lib/firebase",
    "_models": "lib/models",
    "_middlewares": "lib/middlewares"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "pre-push": "yarn lint"
    }
  },
  "private": true
}
